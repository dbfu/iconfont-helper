{
	"name": "iconfont-helper",
	"displayName": "iconfont插件",
	"description": "快速使用iconfont vscode插件",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.77.0"
	},
	"publisher": "dbfu321",
	"categories": [
		"Other"
	],
	"keywords": [
		"iconfont",
		"图标"
	],
	"repository": {
		"url": "https://github.com/dbfu/iconfont-helper"
	},
	"bugs": {
		"url": "https://github.com/dbfu/iconfont-helper/issues"
	},
	"icon": "iconfont.png",
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "iconfont-helper.select_icon",
				"title": "插入iconfont图标"
			}
		],
		"keybindings": [
			{
				"command": "iconfont-helper.select_icon",
				"mac": "cmd+shift+i",
				"win": "ctrl+shift+t",
				"when": "resourceLangId == vue || resourceLangId == javascript || resourceLangId == javascriptreact || resourceLangId == typescript || resourceLangId == typescriptreact"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "resourceLangId == vue || resourceLangId == javascript || resourceLangId == javascriptreact || resourceLangId == typescript || resourceLangId == typescriptreact",
					"command": "iconfont-helper.select_icon",
					"group": "navigation@6"
				}
			]
		},
		"configuration": [
			{
				"title": "iconfont-helper",
				"properties": {
					"iconfont.cookie": {
						"type": "string",
						"description": "cookie"
					},
					"iconfont.fileType": {
						"type": "string",
						"description": "icon文件后缀",
						"default": "tsx"
					},
					"iconfont.dirPath": {
						"type": "string",
						"description": "icon文件夹地址",
						"default": "/src/assets/icons"
					},
					"iconfont.importPath": {
						"type": "string",
						"description": "icon导入地址",
						"default": "@/assets/icons"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "rm -rf ./out && tsc -p ./ && mkdir ./out/src && cp -R ./src/html ./out/src/",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/babel__core": "^7.20.0",
		"@types/ejs": "^3.1.2",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "~18.15.11",
		"@types/prettier": "^2.7.2",
		"@types/react": "^18.0.34",
		"@types/react-dom": "^18.0.11",
		"@types/vscode": "^1.77.0",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"@vscode/test-electron": "^2.1.2",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"@babel/core": "^7.21.4",
		"@babel/generator": "^7.21.4",
		"@babel/parser": "^7.21.4",
		"@babel/preset-react": "^7.18.6",
		"@babel/traverse": "^7.21.4",
		"@babel/types": "^7.21.4",
		"@types/babel__generator": "^7.6.4",
		"@types/babel__traverse": "^7.18.3",
		"@vue/compiler-dom": "^3.2.47",
		"axios": "^1.3.5",
		"babel-plugin-jsx": "^1.2.0",
		"ejs": "^3.1.9",
		"esbuild": "^0.17.16",
		"prettier": "^2.8.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"license": "MIT"
}